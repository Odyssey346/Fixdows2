<?xml version="1.0" encoding="utf-8"?>
<UserControl
    x:Class="Fixdows2.Controls.OutputControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:helpers="using:Fixdows2.Controls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

   <StackPanel Orientation="Vertical" Spacing="20" x:Name="SomeShit" Background="{ThemeResource CardBackgroundFillColorDefault}" Padding="20,20,20,20" CornerRadius="5">
            <TextBlock Text="Output" Style="{ThemeResource SubtitleTextBlockStyle}"/>
            <TextBlock x:Name="TheOutput" TextWrapping="Wrap" Text="{Binding Path=Output}"  />
   </StackPanel>
</UserControl>
